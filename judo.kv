BoxLayout:
    orientation: 'vertical'
    spacing: 10
    padding: 40
    Image:
        source: 'Images/Inhoudstabel4.jpg'
        size: self.texture_size
    Button:
        size_hint: (0.5,0.5)
        pos_hint: {'center_x':0.5,'center_y':0.5}
        text: 'Start'
        on_press: self.calculator
        on_release: ''
    Button:
        size_hint: (0.5,0.5)
        pos_hint: {'center_x':0.5,'center_y':0.5}
        text: 'Profile'
        on_press: ''
    Button:
        size_hint: (0.5,0.5)
        pos_hint: {'center_x':0.5,'center_y':0.5}
        text: 'Settings'
        on_press: ''